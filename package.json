{
  "name": "school-timetable",
  "version": "2.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "dev:e2e": "dotenv -e .env.test -- next dev -p 3000",
    "build": "next build",
    "start": "next start",
    "typecheck": "tsc -p tsconfig.typecheck.json",
    "typecheck:watch": "tsc -w -p tsconfig.typecheck.json",
    "lint": "pnpm run lint:eslint",
    "lint:eslint": "eslint \"src/features/**\" \"src/lib/**\" \"src/types/**\" \"src/utils/**\" --ext .ts,.tsx --report-unused-disable-directives",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write .",
    "test": "pnpm run test:e2e",
    "test:watch": "jest --watchAll",
    "test:unit": "dotenv -e .env.test.local -- jest",
    "test:unit:watch": "dotenv -e .env.test.local -- jest --watchAll",
    "test:db:up": "docker-compose -f docker-compose.test.yml up -d",
    "test:db:down": "docker-compose -f docker-compose.test.yml down",
    "test:db:restart": "docker-compose -f docker-compose.test.yml restart",
    "test:db:logs": "docker-compose -f docker-compose.test.yml logs -f postgres-test",
    "test:db:migrate": "dotenv -e .env.test -- prisma migrate deploy",
    "test:db:reset": "dotenv -e .env.test -- prisma migrate reset --force",
    "test:db:seed": "dotenv -e .env.test -- tsx prisma/seed.ts",
    "test:e2e": "node scripts/run-e2e-tests.mjs",
    "test:e2e:manual": "playwright test",
    "test:e2e:ui": "node scripts/run-e2e-tests.mjs --ui",
    "test:e2e:headed": "node scripts/run-e2e-tests.mjs --headed",
    "test:e2e:debug": "node scripts/run-e2e-tests.mjs --debug",
    "test:e2e:chromium": "playwright test --project=chromium",
    "test:smoke": "playwright test e2e/smoke",
    "test:smoke:ui": "playwright test e2e/smoke --ui",
    "test:smoke:headed": "playwright test e2e/smoke --headed",
    "test:smoke:critical": "playwright test e2e/smoke/critical-smoke.spec.ts",
    "test:smoke:crud": "playwright test e2e/smoke/crud-smoke.spec.ts",
    "test:vercel": "playwright test -c playwright.vercel.config.ts",
    "test:vercel:dashboard": "playwright test -c playwright.vercel.config.ts e2e/integration/analytics-dashboard-vercel.spec.ts",
    "test:vercel:ui": "playwright test -c playwright.vercel.config.ts --ui",
    "test:report": "playwright show-report",
    "playwright:install": "playwright install --with-deps chromium",
    "postinstall": "prisma generate",
    "db:seed": "tsx prisma/seed.ts",
    "db:seed:clean": "SEED_CLEAN_DATA=true tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "db:migrate": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy",
    "admin:create": "tsx scripts/create-admin.ts",
    "admin:verify": "tsx scripts/verify-admin.ts",
    "seed:prod": "pwsh -File scripts/seed-production.ps1",
    "seed:setup": "pwsh -File scripts/add-seed-secret.ps1"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.5",
    "@mui/material": "^7.3.5",
    "@mui/material-nextjs": "^7.3.5",
    "@prisma/adapter-pg": "^7.0.0",
    "@prisma/client": "^7.0.1",
    "@prisma/extension-accelerate": "^2.0.2",
    "@vercel/edge-config": "^1.4.3",
    "@vercel/speed-insights": "^1.2.0",
    "axios": "^1.13.2",
    "bcryptjs": "^3.0.3",
    "better-auth": "^1.4.2",
    "exceljs": "^4.4.0",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.3",
    "next": "16.0.4",
    "next-devtools-mcp": "latest",
    "notistack": "^3.0.2",
    "pg": "^8.16.3",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-icons": "^5.5.0",
    "react-to-print": "^3.2.0",
    "recharts": "^3.4.1",
    "swr": "^2.3.6",
    "valibot": "^1.1.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.28.5",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.39.1",
    "@jest/globals": "^30.2.0",
    "@playwright/test": "^1.56.1",
    "@tailwindcss/postcss": "^4.1.17",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.1",
    "@types/pg": "^8.15.6",
    "@types/react": "19.2.6",
    "@types/react-dom": "19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "autoprefixer": "^10.4.22",
    "babel-plugin-react-compiler": "^1.0.0",
    "dotenv": "^17.2.3",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9.39.1",
    "eslint-config-next": "^16.0.3",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jest": "^29.1.0",
    "eslint-plugin-jest-dom": "^5.5.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-testing-library": "^7.13.4",
    "globals": "^16.5.0",
    "immer": "^10.2.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-mock-extended": "4.0.0",
    "playwright": "^1.56.1",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prisma": "^7.0.1",
    "prisma-dbml-generator": "^0.12.0",
    "rimraf": "^6.1.0",
    "tailwindcss": "^4.1.17",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.47.0",
    "undici": "^7.16.0"
  },
  "prettier": {
    "semi": true,
    "singleQuote": false,
    "trailingComma": "all",
    "bracketSameLine": false
  },
  "packageManager": "pnpm@10.23.0",
  "pnpm": {
    "overrides": {
      "@types/react": "19.2.2",
      "@types/react-dom": "19.2.2"
    },
    "onlyBuiltDependencies": [
      "@vercel/speed-insights"
    ]
  }
}
{
  "name": "school-timetable",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch",
    "lint": "pnpm run lint:eslint",
    "lint:eslint": "eslint . --ext .ts,.tsx --report-unused-disable-directives --max-warnings=0",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write .",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:unit": "dotenv -e .env.test.local -- jest",
    "test:unit:watch": "dotenv -e .env.test.local -- jest --watchAll",
    "test:db:up": "docker-compose -f docker-compose.test.yml up -d",
    "test:db:down": "docker-compose -f docker-compose.test.yml down",
    "test:db:restart": "docker-compose -f docker-compose.test.yml restart",
    "test:db:logs": "docker-compose -f docker-compose.test.yml logs -f postgres-test",
    "test:db:migrate": "dotenv -e .env.test.local -- prisma migrate deploy",
    "test:db:reset": "dotenv -e .env.test.local -- prisma migrate reset --force",
    "test:db:seed": "dotenv -e .env.test.local -- tsx prisma/seed.ts",
    "test:db:setup": "pwsh -File scripts/setup-test-db.ps1",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:brave": "playwright test --project=brave",
    "test:e2e:chromium": "playwright test --project=chromium",
    "test:e2e:admin": "playwright test e2e/admin-auth-flow.spec.ts",
    "test:e2e:admin:brave": "playwright test e2e/admin-auth-flow.spec.ts --project=brave --headed",
    "test:e2e:admin:debug": "playwright test e2e/admin-auth-flow.spec.ts --debug",
    "test:vercel": "playwright test -c playwright.vercel.config.ts",
    "test:vercel:public": "playwright test -c playwright.vercel.config.ts e2e/integration/public-pages.spec.ts",
    "test:vercel:dashboard": "playwright test -c playwright.vercel.config.ts e2e/integration/analytics-dashboard-vercel.spec.ts",
    "test:vercel:ui": "playwright test -c playwright.vercel.config.ts --ui",
    "test:vercel:chromium": "playwright test -c playwright.vercel.config.ts --project=chromium",
    "test:vercel:report": "playwright show-report playwright-report-vercel",
    "playwright:install": "playwright install --with-deps chromium",
    "test:report": "playwright show-report",
    "postinstall": "prisma generate",
    "db:seed": "tsx prisma/seed.ts",
    "db:seed:clean": "SEED_CLEAN_DATA=true tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "db:migrate": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy",
    "db:verify-configid": "tsx scripts/verify-configid-migration.ts",
    "db:migrate-configid": "tsx scripts/migrate-configid.ts",
    "admin:create": "tsx scripts/create-admin.ts",
    "admin:verify": "tsx scripts/verify-admin.ts",
    "seed:prod": "pwsh -File scripts/seed-production.ps1",
    "seed:setup": "pwsh -File scripts/add-seed-secret.ps1"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.11.1",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.4",
    "@mui/material": "^7.3.4",
    "@mui/material-nextjs": "^7.3.3",
    "@prisma/client": "^6.18.0",
    "@prisma/extension-accelerate": "^2.0.2",
    "@sentry/nextjs": "^10.22.0",
    "@vercel/edge-config": "^1.4.3",
    "@vercel/speed-insights": "^1.2.0",
    "axios": "^1.12.2",
    "bcryptjs": "^3.0.2",
    "exceljs": "^4.4.0",
    "fstream": "^1.0.12",
    "import-in-the-middle": "1.15.0",
    "next": "16.0.1",
    "next-auth": "5.0.0-beta.29",
    "next-devtools-mcp": "^0.2.1",
    "notistack": "^3.0.2",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-icons": "^4.12.0",
    "react-to-print": "^3.2.0",
    "recharts": "^3.3.0",
    "require-in-the-middle": "7.5.2",
    "rimraf": "^6.0.1",
    "swr": "^2.3.6",
    "valibot": "^1.1.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.38.0",
    "@playwright/test": "^1.56.1",
    "@tailwindcss/postcss": "^4.1.14",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.19.19",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "autoprefixer": "^10.4.21",
    "babel-plugin-react-compiler": "^1.0.0",
    "dotenv": "^17.2.3",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9.38.0",
    "eslint-config-next": "16.0.0",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-jest-dom": "^5.5.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-testing-library": "^6.5.0",
    "immer": "^10.1.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "playwright": "^1.56.1",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prisma": "^6.18.0",
    "prisma-dbml-generator": "^0.10.0",
    "tailwindcss": "^4.1.14",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "latest",
    "typescript-eslint": "^8.46.2",
    "undici": "^7.16.0"
  },
  "prettier": {
    "semi": true,
    "singleQuote": false,
    "trailingComma": "all",
    "bracketSameLine": false
  },
  "packageManager": "pnpm@10.20.0+sha512.cf9998222162dd85864d0a8102e7892e7ba4ceadebbf5a31f9c2fce48dfce317a9c53b9f6464d1ef9042cba2e02ae02a9f7c143a2b438cd93c91840f0192b9dd",
  "pnpm": {
    "overrides": {
      "@types/react": "19.2.2",
      "@types/react-dom": "19.2.2"
    },
    "onlyBuiltDependencies": [
      "@vercel/speed-insights"
    ]
  }
}

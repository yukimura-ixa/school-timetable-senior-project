{
  "name": "school-timetable",
  "version": "2.0.0",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.0.0",
  "scripts": {
    "dev": "next dev",
    "dev:e2e": "dotenv -e .env.test -- next dev -p 3000",
    "dev:test:local": "dotenv -e .env.test.local -- next dev -p 3000",
    "build": "next build",
    "start": "next start",
    "typecheck": "tsc -p tsconfig.typecheck.json",
    "typecheck:watch": "tsc -w -p tsconfig.typecheck.json",
    "lint": "pnpm run lint:eslint",
    "lint:eslint": "eslint \"src/features/**\" \"src/lib/**\" \"src/types/**\" \"src/utils/**\" --ext .ts,.tsx --report-unused-disable-directives",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write .",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:db:up": "docker-compose -f docker-compose.test.yml up -d",
    "test:db:down": "docker-compose -f docker-compose.test.yml down",
    "test:db:restart": "docker-compose -f docker-compose.test.yml restart",
    "test:db:logs": "docker-compose -f docker-compose.test.yml logs -f postgres-test",
    "test:db:migrate": "dotenv -e .env.test -- prisma migrate deploy",
    "test:db:reset": "dotenv -e .env.test -- prisma migrate reset --force",
    "test:db:seed": "dotenv -e .env.test -- tsx prisma/seed.ts",
    "test:e2e": "node scripts/run-e2e-tests.mjs",
    "test:e2e:manual": "playwright test",
    "test:e2e:ui": "node scripts/run-e2e-tests.mjs --ui",
    "test:e2e:headed": "node scripts/run-e2e-tests.mjs --headed",
    "test:e2e:debug": "node scripts/run-e2e-tests.mjs --debug",
    "test:e2e:chromium": "playwright test --project=chromium",
    "test:smoke": "playwright test e2e/smoke",
    "test:smoke:ui": "playwright test e2e/smoke --ui",
    "test:smoke:headed": "playwright test e2e/smoke --headed",
    "test:smoke:critical": "playwright test e2e/smoke/critical-smoke.spec.ts",
    "test:smoke:crud": "playwright test e2e/smoke/crud-smoke.spec.ts",
    "test:prod:visual": "playwright test --config=playwright.config.prod.ts",
    "test:prod:visual:ui": "playwright test --config=playwright.config.prod.ts --ui",
    "test:prod:visual:update": "playwright test --config=playwright.config.prod.ts --update-snapshots",
    "test:prod:scheduling": "playwright test --config=playwright.config.prod.ts e2e/prod/scheduling",
    "test:prod:crud": "playwright test --config=playwright.config.prod.ts --project=prod-desktop-admin-crud",
    "test:prod:visual:report": "playwright show-report playwright-report-prod",
    "test:report": "playwright show-report",
    "test:e2e:ci-mode": "pnpm build && CI=true pnpm test:e2e:manual",
    "ci:show-trace": "pnpm exec playwright show-trace",
    "playwright:install": "playwright install --with-deps chromium",
    "postinstall": "prisma generate",
    "db:seed": "tsx prisma/seed.ts",
    "db:seed:demo": "dotenv -v SEED_DEMO_DATA=true -- tsx prisma/seed.ts",
    "db:seed:clean": "dotenv -v SEED_CLEAN_DATA=true -- tsx prisma/seed.ts",
    "db:seed:full": "tsx scripts/seed-semester-full.ts",
    "db:studio": "prisma studio",
    "db:migrate": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy",
    "admin:create": "tsx scripts/create-admin.ts",
    "admin:verify": "tsx scripts/verify-admin.ts"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.6",
    "@mui/material": "^7.3.6",
    "@mui/material-nextjs": "^7.3.6",
    "@mui/x-data-grid": "^8.22.1",
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "@prisma/extension-accelerate": "^3.0.1",
    "@react-pdf/renderer": "^4.3.1",
    "@vercel/edge-config": "^1.4.3",
    "@vercel/speed-insights": "^1.3.1",
    "axios": "^1.13.2",
    "better-auth": "^1.4.7",
    "exceljs": "^4.4.0",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.4",
    "next": "16.1.0",
    "next-devtools-mcp": "latest",
    "nodemailer": "^7.0.12",
    "notistack": "^3.0.2",
    "pg": "^8.16.3",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-icons": "^5.5.0",
    "recharts": "^3.6.0",
    "swr": "^2.3.8",
    "valibot": "^1.2.0",
    "vercel": "^50.1.3",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.28.5",
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@playwright/test": "^1.57.0",
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^25.0.3",
    "@types/nodemailer": "^7.0.4",
    "@types/pg": "^8.16.0",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "@typescript-eslint/eslint-plugin": "^8.50.1",
    "@typescript-eslint/parser": "^8.50.1",
    "@vitest/coverage-v8": "^4.0.16",
    "@vitest/ui": "^4.0.16",
    "autoprefixer": "^10.4.23",
    "babel-plugin-react-compiler": "^1.0.0",
    "dotenv": "^17.2.3",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9.39.2",
    "eslint-config-next": "^16.1.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-testing-library": "^7.15.3",
    "globals": "^16.5.0",
    "happy-dom": "^20.0.11",
    "immer": "^11.1.0",
    "playwright": "^1.57.0",
    "postcss": "^8.5.6",
    "prettier": "^3.7.4",
    "prisma": "^7.2.0",
    "prisma-dbml-generator": "^0.12.0",
    "rimraf": "^6.1.2",
    "tailwindcss": "^4.1.18",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.1",
    "undici": "^7.16.0",
    "vitest": "^4.0.16"
  },
  "prettier": {
    "semi": true,
    "singleQuote": false,
    "trailingComma": "all",
    "bracketSameLine": false
  },
  "packageManager": "pnpm@10.26.1+sha512.664074abc367d2c9324fdc18037097ce0a8f126034160f709928e9e9f95d98714347044e5c3164d65bd5da6c59c6be362b107546292a8eecb7999196e5ce58fa",
  "pnpm": {
    "overrides": {
      "@types/react": "19.2.2",
      "@types/react-dom": "19.2.2"
    },
    "onlyBuiltDependencies": [
      "@vercel/speed-insights",
      "esbuild"
    ]
  }
}